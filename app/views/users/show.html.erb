<table class="table font-table">
  <thead>
    <tr>
      <th scope="col" class="laber">
        <%= t "form.image" %>
      </th>
      <th class="image">
        <%= gravatar_for @user %>
      </th>
    </tr>
    <tr>
      <th scope="col" class="laber">
        <%= t "form.full_name" %>
      </th>
      <th>
        <%= @user.name %>
      </th>
    </tr>
    <tr>
      <th scope="col" class="laber">
        <%= t "form.email" %>
      </th>
      <th>
       <%= @user.email %>
      </th>
    </tr>
    <tr>
      <th scope="col" class="laber">
        <%= t "form.created_at" %>
      </th>
      <th>
     <%= l @user.created_at, format: :long %><br/>
      </th>
    </tr>
    <tr>
      <th scope="col" class="laber">
        <%= t "form.updated_at" %>
      </th>
      <th>
        <%= l @user.updated_at, format: :long %></h6>
      </th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>
